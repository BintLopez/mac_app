<%= form_for :on_call_time, remote: true do |f| %>
  Start Time: <%= date_select :start_time, :start_time, :class => 'datepicker' %>
  End Time : <%= date_select :end_time, :end_time, :class => 'datepicker' %>
  Volunteer : <%= f.collection_select :provider_id, User.order(:first_name),:id,:first_name, include_blank: true %>
  <%= f.submit %>
<% end %>