<%= form_for :on_call_time do |f| %>
  Start Time: <%= date_select :start_time, :start_time, :class => 'datepicker' %>
  End Time : <%= date_select :end_time, :end_time, :class => 'datepicker' %>
  Volunteer : <%= f.collection_select :provider_id, available_on_call_volunteers,:id,:first_name, include_blank: true %>
  <%= f.submit %>
<% end %>